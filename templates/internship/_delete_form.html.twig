<form method="post" action="{{ path('app_internship_delete', { id: internship.id }) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
  <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ internship.id) }}" />
  <button class="btn btn-danger">{{ 'global.btn_delete'|trans }}</button>
</form>
