{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "0.2.0",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "dompdf/dompdf": "^3.0",
        "endroid/qr-code-bundle": "^5.0",
        "lcobucci/jwt": "^5.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.27",
        "symfony/asset": "7.*",
        "symfony/asset-mapper": "7.*",
        "symfony/console": "7.*",
        "symfony/doctrine-messenger": "7.*",
        "symfony/dotenv": "7.*",
        "symfony/expression-language": "7.*",
        "symfony/filesystem": "7.*",
        "symfony/flex": "^2",
        "symfony/form": "7.*",
        "symfony/framework-bundle": "7.*",
        "symfony/http-client": "7.*",
        "symfony/intl": "7.*",
        "symfony/lock": "7.*",
        "symfony/mailer": "7.*",
        "symfony/mercure-bundle": "^0.3.9",
        "symfony/messenger": "7.*",
        "symfony/mime": "7.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.*",
        "symfony/process": "7.*",
        "symfony/property-access": "7.*",
        "symfony/property-info": "7.*",
        "symfony/rate-limiter": "7.*",
        "symfony/runtime": "7.*",
        "symfony/security-bundle": "7.*",
        "symfony/serializer": "7.*",
        "symfony/stimulus-bundle": "^2.16",
        "symfony/string": "7.*",
        "symfony/translation": "7.*",
        "symfony/twig-bundle": "7.*",
        "symfony/uid": "7.*",
        "symfony/ux-turbo": "^2.16",
        "symfony/validator": "7.*",
        "symfony/web-link": "7.*",
        "symfony/yaml": "7.*",
        "tattali/calendar-bundle": "^1.3",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.10",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.*"
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.6",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.*",
        "symfony/css-selector": "7.*",
        "symfony/debug-bundle": "7.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/panther": "^2.1",
        "symfony/phpunit-bridge": "^7",
        "symfony/stopwatch": "7.*",
        "symfony/web-profiler-bundle": "7.*"
    }
}
